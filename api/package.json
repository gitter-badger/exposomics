{
  "name": "@doc.ai/exposomics-api",
  "version": "1.0.0",
  "description": "Discover things you are exposed to",
  "main": "index.js",
  "repository": "git@github.com:doc-ai/exposomics.git",
  "author": "doc.ai Team <dev@doc.ai>",
  "license": "GPL-3.0",
  "contributors": [
    {
      "name": "@doc.ai/exposomics contributors",
      "url": "https://github.com/doc-ai/exposomics/graphs/contributors"
    }
  ],
  "scripts": {
    "dev": "nodemon index.js",
    "babel": "babel src -d build",
    "build": "npm run clean && npm run babel",
    "start": "node build/index.js",
    "clean": "rimraf build",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "flow && lint-staged",
    "db:drop": "sequelize db:drop",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:seed:all": "sequelize db:seed:all"
  },
  "lint-staged": {
    "src/**/*.{js,mjs}": [
      "eslint"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": true
  },
  "dependencies": {
    "@doc.ai/exposomics-common": "../common",
    "dotenv": "^5.0.0",
    "http-errors": "^1.6.2",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.4.0",
    "koa2-cors": "^2.0.5",
    "moment": "^2.20.1",
    "mysql2": "^1.5.2",
    "rc": "^1.2.5",
    "sequelize": "^4.32.7",
    "sequelize-cli": "^3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.39",
    "@babel/core": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/preset-stage-0": "^7.0.0-beta.39",
    "@babel/register": "^7.0.0-beta.39",
    "babel-eslint": "^8.0.2",
    "babel-preset-flow": "^7.0.0-beta.3",
    "bytes": "^3.0.0",
    "csv": "^2.0.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "event-stream": "^3.3.4",
    "flow-bin": "^0.58.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "nodemon": "^1.14.12",
    "prettier": "^1.7.4",
    "progress": "^2.0.0",
    "request": "^2.83.0",
    "request-progress": "^3.0.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.0",
    "unzip": "^0.1.11"
  }
