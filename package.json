{
  "name": "@doc.ai/exposomics",
  "version": "1.0.0",
  "description": "Discover things you are exposed to",
  "main": "server.js",
  "repository": "git@github.com:doc-ai/exposomics.git",
  "author": "doc.ai Team <dev@doc.ai>",
  "license": "GPL-3.0",
  "contributors": [
    {
      "name": "@doc.ai/exposomics contributors",
      "url": "https://github.com/doc-ai/exposomics/graphs/contributors"
    }
  ],
  "scripts": {
    "install:api": "cd api && yarn",
    "install:common": "cd common && yarn",
    "install:frontend": "cd frontend && yarn",
    "install:proxy": "cd proxy && yarn",
    "build:common": "cd common && yarn build",
    "build:api": "cd api && yarn build",
    "build:frontend": "cd frontend && yarn build",
    "db:create": "cd api && yarn db:create",
    "db:migrate": "cd api && yarn db:migrate",
    "db:seed": "cd api && yarn db:seed:all",
    "postinstall": "yarn install:api && yarn install:common && yarn install:frontend && yarn install:proxy",
    "dev": "pm2-docker ecosystem.config.js",
    "start": "pm2-docker ecosystem-prod.config.js"
  },
